[run]
source = .
omit = 
    .venv/*
    tests/*
    scripts/*
    */__pycache__/*
    */migrations/*
    setup.py
    conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = scripts/test_reports/coverage
